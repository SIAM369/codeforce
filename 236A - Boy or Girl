#include <bits/stdc++.h>
using namespace std;

int main (){
  string a;
  cin>>a;
  int c=0;
  
  sort(a.begin(), a.end());
  for(int i=0; i<a.size(); i++){
      if(a[i]!=a[i+1]){
          c++;
      }
  }
  if(c%2==0){
      cout<<"CHAT WITH HER!"<<endl;
  }
  else{
      cout<<"IGNORE HIM!"<<endl;
  }
}
